#ifndef __NAUNET_ENZO_H__
#define __NAUNET_ENZO_H__
// clang-format off
#include "naunet.h"
#include "naunet_data.h"

#define NAUNET_SPECIES 4
// number of equations to be solved
#define NAUNET_NSPECIES {{ nspec }}
#define NAUNET_NEQUATIONS {{ neqns }}


{% for s in species -%}
#define A_{{ s.alias }} {{ 1.0 if s.iselectron else s.massnumber }}
{% endfor %}


const float A_Table[NSPECIES] = {
    {% for s in species -%}
    A_{{ s.alias }}{{ ",\n    " if not loop.last else "\n" }}
    {%- endfor -%}
};

#endif
